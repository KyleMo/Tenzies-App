Change(e){\n    setExtreme(false)\n    if(e.target.value === \"30e\"){\n      setExtreme(true)\n      setDifficulty(30)\n    } else {\n      setDifficulty(e.target.value)\n    }\n    setViewSettings(prev => !prev)\n  }\n\n  function Settings(){\n    return(\n      <div className=\"settings\">\n        <h2>Difficulty</h2>\n        <div className=\"settings-difficulty\">\n          <button className=\"setting-diff-button\" onClick={handleChange} value={10}>Easy</button>\n          <button className=\"setting-diff-button\" onClick={handleChange} value={20}>Medium</button>\n          <button className=\"setting-diff-button\" onClick={handleChange} value={30}>Hard</button>\n          <button className=\"setting-diff-button\" onClick={handleChange} value={\"30e\"}>Extreme</button>\n        </div>\n      </div>\n    )\n  }\n\n  return(\n    <div className={difficulty>29?\"tenzies-app-2\":\"tenzies-app\"}>\n      <div className=\"settings-button\" onClick={handleClick}>{viewSetting?<span className=\"settings-span\"><span className=\"settings-gear\">&#8592;</span>Back</span>:<span className=\"settings-span\"><span className=\"settings-gear\">&#9881;</span>Settings</span>}</div>\n      {viewSetting?<Settings />:<Tenzies extreme={extreme} difficulty={difficulty}/>}\n      <Link to=\"/leaderboards\" className=\"leaderboards-link\">Leaderboards</Link>\n    </div>\n  )\n}\n\nexport default TenziesApp;\n","/Users/kylemonstad/Desktop/Tenzies/client/src/components/Leaderboards/Leaderboards.js",["89"],[],"import React from 'react'\nimport './leaderboard.css'\nimport { useNavigate } from 'react-router-dom'\nimport axios from 'axios'\n\nconst Leaderboards = () => {\n\n  //api call for easy, medium, hard, and extreme games difficulties\n  const [matchDiffFilter, setMatchDiffFilter] = React.useState('Easy')\n  const [error, setError] = React.useState(null)\n  const [savedTimes, setSavedTimes] = React.useState([])\n  const navigate = useNavigate()\n\n  React.useEffect(() => {\n    async function fetchData(){\n      try {\n        const response = await axios.get(`/api/leaderboard/get?difficulty=${matchDiffFilter}`)\n        setSavedTimes(response.data)\n      } catch (e) {\n        setError(e.response.data.message)\n      }\n    }\n    fetchData()\n\n  }, [matchDiffFilter])\n\n  //Event functions\n  const goBack = () => {\n    navigate('/');\n  }\n\n  const handleChange = (e) => {\n    setMatchDiffFilter(e.target.value)\n  }\n\n  const tableData = savedTimes\n    .map((match,index) => {\n    return (\n      <tr className=\"table-row\" key={match.id}>\n        <td>{index+1}</td>\n        <td>{match.username}</td>\n        <td>{match.difficulty}</td>\n        <td>\n          <span>{(\"0\" + Math.floor((match.time / 60000) % 60)).slice(-2)}:</span>\n          <span>{(\"0\" + Math.floor((match.time / 1000) % 60)).slice(-2)}:</span>\n          <span>{(\"0\" + ((match.time / 10) % 100)).slice(-2)}</span>\n        </td>\n      </tr>\n    )\n  })\n\n  return (\n    <div className=\"leaderboards\">\n      <div onClick={goBack} className=\"back\">&#8592; Back</div>\n      <h1>Leaderboards</h1>\n      <div className=\"leaderboard-filter\">\n        <select onChange={handleChange}>\n          <option value=\"Easy\">Easy</option>\n          <option value=\"Medium\">Medium</option>\n          <option value=\"Hard\">Hard</option>\n          <option value=\"Extreme\">Extreme</option>\n        </select>\n      </div>\n      <div className=\"table-container\">\n        <table className=\"best-game-table\">\n          <thead>\n            <tr>\n              <th>Rank</th>\n              <th>Name</th>\n              <th>Difficulty</th>\n              <th>Time</th>\n            </tr>\n          </thead>\n          <tbody>{tableData}</tbody>\n        </table>\n      </div>\n\n    </div>\n  )\n}\n\nexport default Leaderboards;\n","/Users/kylemonstad/Desktop/Tenzies/client/src/components/TenziesGame/TenziesGame.js",["90","91","92","93","94"],[],"/Users/kylemonstad/Desktop/